<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tao.mychat"
    android:versionCode="1"
    android:versionName="1.0"
    >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.launcher.permission.CHANGE_BADGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.LoginActivity"></activity>
        <activity android:name=".app.home.HomeActivity"
            ></activity>
        <activity android:name=".app.RegisterActivity"></activity>

        <activity android:name=".app.setting.user.UserEditActivity"></activity>
        <activity android:name=".app.album.AblumActivity"></activity>
        <activity android:name=".app.exchangeskin.ExchangeSkinActicity"></activity>
        <activity android:name=".app.setting.about.AboutActivity"></activity>
        <activity android:name=".app.wallet.WalletActivity"></activity>
        <activity android:name=".app.expression.ExpressionActivity"></activity>
        <activity android:name=".app.setting.remind.RemindActivity"></activity>
        <activity android:name=".app.setting.privateandsafe.PrivateAndSafe"></activity>
        <activity android:name=".app.setting.nodistrub.NoDistrubActivity"></activity>
        <activity android:name=".app.setting.normal.NormalActivity"></activity>
        <activity android:name=".app.setting.statistics.StatisticalFlowActivity"></activity>
    </application>


</manifest>